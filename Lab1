import java.util.Scanner;

public class Main {


		 public static void main(String[] args) {
		 
		
		    {
		        Scanner in = new Scanner(System.in);
		        System.out.print("Input the first number for cameroon: ");
		        double x = 60;
		        System.out.print("Input the second number Moroco: ");
		        double y = 80;
		        System.out.print("Input the third number for jamaica: ");
		        double z = 100;
		        System.out.print("The average value is " + average(x, y, z)+"\n" );}
		    }
		 public static double average(double x, double y, double z)
		    {
		        return ( x + y + z) / 3;
	
}
		 }
